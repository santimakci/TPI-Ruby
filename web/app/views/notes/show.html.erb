<br><br><br>
<div class="row">

    <div class="col s6 offset-s3">
    <div class="card">
    <div class="card-panel z-depth-5">

        <!-- Form Username Input Section -->

        <div class="col s12 m12 l12">
            <div class="input-field">
                <i class="material-icons prefix">title</i>
                   <input disabled value="<%= @note.title %>" > </input>
            </div>
        </div>
        <div class="col s12 m12 l12">
            <div class="input-field">
                <i class="material-icons prefix">format_color_text</i>
                <textarea disabled class="input-size" type="text" value="" > <%= @note.text %></textarea>
            </div>
        </div>
           <div class="col s12 m12 l12">
            <div class="input-field">
                <i class="material-icons prefix">web</i>
                
                <textarea  disabled style="height: 250px;" class="input-size" type="text" value="" > <%= @htmlfinal %></textarea>
            </div>
        </div>
        <div class="center">
             <%= link_to 'Exportar', notes_download_path(@note), class:'waves-effect waves-light btn buton-export', method: :post%>
              <%= link_to 'Editar', edit_note_path(@note), class:'waves-effect waves-light btn buton-export' %>
        </div>



    </div>
    </div>
</div>
